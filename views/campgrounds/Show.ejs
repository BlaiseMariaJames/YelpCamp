<% layout('/layouts/BoilerPlate') %>
    <h1>
        <%=campground.title%>
    </h1>
    <p>
        Registrarion Id : <%=campground._id %>
    </p>
    <p>
        Price : Rs. <%=campground.price %>/-
    </p>
    <p>
        Location : <%=campground.location %>
    </p>
    <p>
        Description : <%= (campground.description) ? campground.description : "None" %>
    </p>
    <button><a href="/campgrounds"><b>Go Back</b></a></button>
    <button><a href="/campgrounds/<%=campground._id%>/edit"><b>Edit</b></a></button>
    <button><a href="/campgrounds/<%=campground._id%>/remove"><b>Delete</b></a></button>